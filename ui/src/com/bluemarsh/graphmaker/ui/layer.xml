        <attr name="View/Tools" boolvalue="true"/>
    </folder>

    <folder name="Templates">
        <folder name="Other">
            <file name="newGraph">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.bluemarsh.graphmaker.ui.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="Other"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/com/bluemarsh/graphmaker/ui/wizard/newFile.html"/>
                <attr name="templateWizardIterator" newvalue="com.bluemarsh.graphmaker.ui.wizard.NewFileWizardIterator"/>
            </file>
        </folder>
    </folder>
